{
	"manifest_version": 2,
    	"name": "TikTok Search",
	"description": "Search TikTok directly from Firefox.",
	"version": "0.1",
	"applications":{
		"gecko": {
			"strict_min_version": "73.0"
		}
	},
	"icons": {
		"32": "icon.png"
	},
  	"chrome_settings_overrides": {
		"search_provider": {
		"name": "TikTok",
            	"keyword": "@TT",
		"search_url": "https://www.tiktok.com/search?q={searchTerms}"
		}
	}
}
